=\"Collection_Description\" placeholder=\"Collection Description\">\n        <input v-model=\"authorized_accounts\" placeholder=\"authorized account \">\n        <b-button @click=\"CreateCollection\">Create Collection</b-button>\n    </div>\n</template>\n\n<script>\n\nimport { mapGetters } from 'vuex'\n\nexport default {\n    name:'CreateCollection',\n    props:['wax','assetID'],\n    data(){\n        return{\n            Collection_name:\"\",\n            Display_name:\"\",\n            WebURL:\"\",\n            Collection_Description:\"\",\n            authorized_accounts:[this.wax.userAccount],\n            notified_accounts:[],\n            market_fee:\"\"\n        }\n    },\n    computed: {\n      ...mapGetters([\n        'getWax'\n      ])\n    },\n    methods:{\n       async CreateCollection(){\n           console.log(this.wax.api);\n           if(!this.wax.api){\n               return console.log(\"Need to login first\")\n           }\n           try {\n               this.result=await this.wax.api.transact({\n                   actions:[{\n                    account: 'atomicassets',\n                    name: 'createcol',\n                    authorization: [{\n                        actor: this.wax.userAccount,\n                        permission:'active',\n                    }],\n                    data: {\n                        author:this.wax.userAccount,\n                        Collection_name:this.Collection_name,\n                        allow_notify:false,\n                        authorized_accounts:this.authorized_accounts,\n                        notified_accounts:this.notified_accounts,\n                        market_fee:this.market_fee,\n                        Collection_Description:this.Collection_Description\n                    },\n                   }]\n               },\n               {\n                   blockBehind: 3,\n                   expireSecond: 30\n               })\n               console.log(this.result)\n               console.log(\"THIS AUTHOR IS \"+this.author);\n           }\n           catch(e){\n               this.result=e\n               console.log(e)\n           }       \n       }\n    }\n}\n    \n</script>\n"]}]}